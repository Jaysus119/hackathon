{
    "manifest_version": 3,
    "name": "RubberDucker",
    "version": "1.0",
    "description": "A classic and powerful debugging tool.",
    "icons": {
        "48": "images/rubberDuck-48.png",
        "128": "images/rubberDuck-128.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["activeTab", "scripting"],
    "web_accessible_resources": [
        {
          "resources": ["script.js", "images/rubberDuck-720.png"],
          "matches": ["<all_urls>"]
        }
    ],
    "action": {
        "default_icon": {
            "48": "images/rubberDuck-48.png",
            "128": "images/rubberDuck-128.png"
          }
    }
  }